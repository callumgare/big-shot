{
  "name": "big-shot",
  "version": "0.4.0",
  "description": "A Vue-based image/video slide show widget",
  "author": "Callum Gare <callum@gare.com.au>",
  "repository": "github:callumgare/big-shot",
  "keywords": [
    "slide-show",
    "slide show",
    "slideshow",
    "vue",
    "photoswipe"
  ],
  "license": "MIT",
  "main": "./dist/big-shot.umd.js",
  "module": "./dist/big-shot.es.js",
  "unpkg": "./dist/big-shot.es.js",
  "exports": {
    ".": {
      "import": "./dist/big-shot.es.js",
      "require": "./dist/big-shot.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build-examples": "npm run build && vite -c examples/vite-for-example-using-es.config.js build",
    "serve-examples": "npx --yes http-server built-examples",
    "lint": "concurrently npm:lint:*",
    "lint-fix": "concurrently npm:lint-fix:*",
    "lint:stylelint": "stylelint '{src,dev}/*.{vue,scss}' ",
    "lint-fix:stylelint": "stylelint '{src,dev}/*.{vue,scss}' --fix",
    "lint:eslint": "eslint --ext .vue,.js src/ dev/",
    "lint-fix:eslint": "eslint --ext .vue,.js --fix src/ dev/",
    "release": "release-it"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.9.4",
    "concurrently": "^6.3.0",
    "eslint": "^8.2.0",
    "eslint-plugin-vue": "^8.0.3",
    "hammerjs": "^2.0.8",
    "minimist": "^1.2.5",
    "mitt": "^3.0.0",
    "postcss-html": "^1.2.0",
    "release-it": "^14.11.6",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "sass": "^1.42.1",
    "stylelint": "^14.0.1",
    "stylelint-config-recommended-vue": "^1.0.0",
    "stylelint-config-standard-scss": "^2.0.1",
    "stylelint-scss": "^4.0.0",
    "stylelint-webpack-plugin": "^3.1.0",
    "stylus": "^0.54.8",
    "vite": "^2.2.3",
    "vite-plugin-singlefile": "^0.5.1",
    "vite-svg-loader": "^3.1.0",
    "vue": "^3.1.1"
  }
}
